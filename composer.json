{
    "name": "stevenbraham/laravel-nominatim-search",
    "description": "Very basic Laravel component for quickly geocoding an address using Nominatim with caching",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "StevenBraham\\NominatimSearch\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Steven Braham",
            "email": "steven@braham.biz"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "ext-json": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "StevenBraham\\NominatimSearch\\NominatimSearchServiceProvider"
            ]
        }
    }
}
